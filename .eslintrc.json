{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "rules": {
        "no-console": "error",
        "linebreak-style": ["error", "windows"],
        "no-underscore-dangle":"off",
        "global-require": "off",
        "no-unused-vars":"off",
        "max-len":"off"
    }
}
